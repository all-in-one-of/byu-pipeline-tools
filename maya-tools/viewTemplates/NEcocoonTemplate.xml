<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<using package='std'/>
	<template name='NEcocoon'>
		<attribute name='outPositionPP' type='maya.vectorArray'>
			<label>Out Position PP</label>
		</attribute>
		<attribute name='outDirectionPP' type='maya.vectorArray'>
			<label>Out Direction PP</label>
		</attribute>
		<attribute name='outScalePP' type='maya.vectorArray'>
			<label>Out Scale PP</label>
		</attribute>
		<attribute name='outRgbaPP' type='maya.doubleArray'>
			<label>Out Rgba PP</label>
		</attribute>
		<attribute name='outThicknessPP' type='maya.doubleArray'>
			<label>Out Thickness PP</label>
		</attribute>
		<attribute name='outLinksMesh' type='maya.mesh'>
			<label>Out Links Mesh</label>
		</attribute>
		<attribute name='outFacesMesh' type='maya.mesh'>
			<label>Out Faces Mesh</label>
		</attribute>
		<attribute name='inPositionPP' type='maya.vectorArray'>
			<label>In Position PP</label>
		</attribute>
		<attribute name='inRefPositionPP' type='maya.vectorArray'>
