<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<using package='std'/>
	<template name='NEpointEmitter'>
		<attribute name='matrix' type='maya.matrix'>
			<label>Matrix</label>
		</attribute>
		<attribute name='inverseMatrix' type='maya.matrix'>
			<label>Inverse Matrix</label>
		</attribute>
		<attribute name='worldMatrix' type='maya.matrix'>
			<label>World Matrix</label>
		</attribute>
		<attribute name='worldInverseMatrix' type='maya.matrix'>
			<label>World Inverse Matrix</label>
		</attribute>
		<attribute name='parentMatrix' type='maya.matrix'>
			<label>Parent Matrix</label>
		</attribute>
		<attribute name='parentInverseMatrix' type='maya.matrix'>
			<label>Parent Inverse Matrix</label>
		</attribute>
		<attribute name='output' type='maya.newParticles'>
			<label>Output</label>
		</attribute>
		<attribute name='translate' type='maya.double3'>
			<label>Translate</label>
		</attribute>
		<attribute name='rotate' type='maya.double3'>
			<label>Rotate</label>
		</attribute>
		<attribute name='scale' type='maya.double3'>
			<label>Scale</label>
		</attribute>
		<attribute name='visibility' type='maya.bool'>
			<label>Visibility</label>
		</attribute>
		<attribute name='isFull' type='maya.bool'>
			<label>Is Full</label>
		</attribute>
		<attribute name='startTime' type='maya.time'>
			<label>Start Time</label>
		</attribute>
		<attribute name='deltaTime' type='maya.time'>
			<label>Delta Time</label>
		</attribute>
		<attribute name='inheritFactor' type='maya.double'>
			<label>Inherit Factor</label>
		</attribute>
		<attribute name='seed' type='maya.long'>
			<label>Seed</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEpointEmitter'>
		<property name='matrix'/>
		<property name='inverseMatrix'/>
		<property name='worldMatrix'/>
		<property name='worldInverseMatrix'/>
		<property name='parentMatrix'/>
		<property name='parentInverseMatrix'/>
		<property name='output'/>
		<property name='translate'/>
		<property name='rotate'/>
		<property name='scale'/>
		<property name='visibility'/>
		<property name='isFull'/>
		<property name='startTime'/>
		<property name='deltaTime'/>
		<property name='inheritFactor'/>
		<property name='seed'/>
	</view>
</templates>